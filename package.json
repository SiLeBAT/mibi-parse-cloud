{
  "name": "mibi-parse-cloud",
  "version": "0.0.1",
  "license": "MIT",
  "private": false,
  "type": "commonjs",
  "scripts": {
    "tsc": "tsc",
    "start": "npm run clean && tsc -w",
    "bundle": "npm run clean && npx esbuild src/main.ts --platform=node --bundle --minify --outfile=dist/main.js",
    "build": "npm run tsc",
    "prebuild": "npm run clean",
    "clean": "rimraf lib/* dist/*",
    "lint": "npx eslint .",
    "lint:fix": "npm run lint -- --fix",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "prepare": "(husky -v && husky install) || echo husky not installed"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run pretty",
      "npm run lint"
    ]
  },
  "dependencies": {
    "config": "~3.3.8",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "~4.17.21",
    "moment": "~2.29.4",
    "parse": "^4.3.1",
    "parse-server": "^6.4.0",
    "shallow-equal-object": "^1.1.1",
    "uuid": "^9.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz",
    "xlsx-populate": "~1.21.0"
  },
  "devDependencies": {
    "@types/config": "3.3.0",
    "@types/jest": "27.5.2",
    "@types/lodash": "4.14.186",
    "@types/node": "16.11.65",
    "@types/parse": "^3.0.4",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "copyfiles": "~2.4.1",
    "eslint": "^8.56.0",
    "husky": "~7.0.4",
    "jest": "~27.5.1",
    "jest-stare": "~2.4.1",
    "lint-staged": "~12.4.3",
    "nodemon": "~2.0.20",
    "prettier": "2.6.2",
    "request": "~2.88.2",
    "rimraf": "~3.0.2",
    "ts-jest": "27.1.5",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "~1.18.0",
    "tslint-config-standard": "9.0.0",
    "typescript": "^4.4.2"
  },
  "overrides": {
    "undici": "5.22.0",
    "semver": "7.5.2",
    "crypto-js": "4.2.0"
  },
